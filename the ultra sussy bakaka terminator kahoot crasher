javascript:(function(){var msg=[0x4E,0x6F,0x74,0x20,0x61,0x6C,0x6C,0x6F,0x77,0x65,0x64];var txt='';for(var i=0;i<msg.length;i++){txt+=String.fromCharCode(msg[i]);}var b=document.createElement('button');b.textContent='autohirizate';b.style='position:fixed;top:50%;left:50%;transform:translate(-50%,-50%);padding:12px 20px;font-size:16px;border-radius:8px;background:#fff;border:2px solid #000;z-index:99999;cursor:pointer;';b.onclick=function(){alert(txt);b.remove();};document.body.appendChild(b);})();
